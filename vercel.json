{
  "version": 2,
  "builds": [
    {
      "src": "vocal_remover_backend.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "vocal_remover_backend.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "functions": {
    "vocal_remover_backend.py": {
      "maxDuration": 300
    }
  }
}